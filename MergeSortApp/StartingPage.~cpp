#include <vcl.h>
#pragma hdrstop

#include "StartingPage.h"
#include "DidacticPage.h"
#include "EducationalPage.h"
#include "AdvancedPage.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.dfm"
TfStartingPage *fStartingPage;
//---------------------------------------------------------------------------
__fastcall TfStartingPage::TfStartingPage(TComponent* Owner)
        : TForm(Owner)
{
}
//---------------------------------------------------------------------------

void __fastcall TfStartingPage::bDidacticPageClick(TObject *Sender)
{
        fStartingPage->Hide();
        fDidacticPage->Show();
}
//---------------------------------------------------------------------------

